<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student2" class="com.kyle.student.Student">
		<constructor-arg value="유관순"></constructor-arg>
		<constructor-arg value="50"></constructor-arg>
		<constructor-arg>
			<list>
				<value>마라톤</value>
				<value>장난감</value>
			</list>
		</constructor-arg>
		<property name="height" value="160"></property>
		<property name="weight" value="50"></property>
	</bean>

</beans>
